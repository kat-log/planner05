<div class="md:flex m-9">
    <% @ranking_by_age.each do |ranking| %>
      <div class="m-9 p-5 basis-1/3 bg-blue-400 rounded-xl text-white">
        <div class="m-5 rounded-xl ">
          <%= link_to image_tag(ranking['mediumImageUrls'][0], 
          { class:"rounded-xl hover:opacity-75 container border border-blue-500"}), 
          ranking.url, target: :_blank, rel: "noopener noreferrer" %>
        </div>
        <%= ranking.catchcopy %>
      </div>
      <%= break if ranking.rank ==3 %>
    <% end %>
</div>