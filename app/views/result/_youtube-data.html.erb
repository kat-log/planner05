<div class="md:flex m-9">
  <% @youtube_data.items.each do |item| %>
    <div class="m-9 mt-0 p-5 basis-1/3 bg-gradient-to-b from-sky-300 to-sky-600 rounded-xl text-white">
      <% snippet = item.snippet %>
      <div class="">
        <iframe class="container mx-auto rounded-xl" height="315" src="https://www.youtube.com/embed/<%= item.id.video_id %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      </div>
      <p><%= snippet.title %></p>
    </div>
  <% end %>
</div>