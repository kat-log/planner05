<div class="md:flex m-9">
    <% @ranking_by_age.each do |ranking| %>
      <div class="m-9 mt-0 p-5 basis-1/3 bg-gradient-to-b from-sky-300 to-sky-600 rounded-xl text-white">
        <div class="m-5 rounded-xl">
          <%= link_to image_tag(ranking['mediumImageUrls'][0], 
          { class:"rounded-xl hover:opacity-75 container border border-blue-500"}), 
          ranking.url, target: :_blank, rel: "noopener noreferrer" %>
        </div>
        <%= ranking.catchcopy %>
      </div>
      <%= break if ranking.rank ==3 %>
    <% end %>
</div>

<div class="md:flex m-9">
  <% @youtube_data.items.each do |item| %>
    <% snippet = item.snippet %>
    <div><iframe width="560" height="315" src="https://www.youtube.com/embed/<%= item.id.video_id %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div>
    <p><%= snippet.title %></p>
  <% end %>
</div>